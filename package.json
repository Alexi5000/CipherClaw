{
  "name": "cipherclaw",
  "version": "1.0.0",
  "description": "An OpenClaw debug agent that traces causes, profiles behavior, and predicts failures in multi-agent systems.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "skills",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openclaw",
    "debug",
    "ai-agent",
    "multi-agent",
    "observability",
    "tracing",
    "causal-inference",
    "cognitive-fingerprinting",
    "cipherclaw"
  ],
  "author": "ClawLI.AI",
  "license": "Apache-2.0",
  "homepage": "https://cipherclaw.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alexi5000/CipherClaw.git"
  },
  "bugs": {
    "url": "https://github.com/Alexi5000/CipherClaw/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "vitest": "^3.0.0"
  }
}
